{
  "compilerOptions": {
  "experimentalDecorators": true,
  "esModuleInterop": true,
  "stripInternal": true,
  "target": "es6",
  "module": "CommonJS",
  "moduleResolution": "node16",
  "resolveJsonModule": true,
  "noImplicitOverride": true,
  "declarationMap": true,
  "declaration": true,
  "outDir": "./dist",
  "jsx": "react-jsx",
  "strict": true
},
"include": ["src"],
"exclude": ["node_modules", "dist", "**/__tests__/*"]
}